// Jary's Heavy Scrake Cycle
class jhscc_v1 extends CD_SpawnCycle_PresetBase
	implements (CD_SpawnCycle_Preset);

function GetShortSpawnCycleDefs( out array<string> sink )
{
	GetLongSpawnCycleDefs( sink );

	// keep waves 1, 4, 7, 10
	sink.Remove(1, 2);
	sink.Remove(2, 2);
	sink.remove(3, 2);

	sink.Length = 4;
}

function GetNormalSpawnCycleDefs( out array<string> sink )
{
	// keep waves 1, 2, 4, 6, 7, 9, 10
    GetLongSpawnCycleDefs(sink);
    sink.Remove(2, 1);
    sink.Remove(3, 1);
    sink.Remove(5, 1);
    sink.Length = 7;
}

